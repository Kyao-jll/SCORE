<head>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
      google.charts.load('current', {'packages':['corechart']});
      google.charts.setOnLoadCallback(drawSeriesChart);

    function drawSeriesChart() {

    var data = google.visualization.arrayToDataTable([
      ['Name', 'Overall_Score', 'Area_Rent', 'Name', 'Square_Footage'],
      <% @list_of_properties.each do |a_prop| %>
      ['<%= a_prop.prop_name %>',<%= a_prop.scores.first.overall_score.to_json %>,<%= a_prop.area_rent.to_json %>,'<%= a_prop.prop_name %>',<%= a_prop.size_sqft.to_json %>],
      <% end %>
      
    ]);
    
    var options = {
      title: 'Area Rent vs Overall_Score' +
        ' X=Overall_Score, Y=Area_Rent, Bubble size=Square_Footage, Bubble color=Name',
      hAxis: {title: 'Overall_Score'},
      vAxis: {title: 'Area_Rent'},
      bubble: {textStyle: {fontSize: 11}}
    };

    var chart = new google.visualization.BubbleChart(document.getElementById('series_chart_div'));
    chart.draw(data, options);

    
  }
  </script>
</head>
<body>
  <div id="series_chart_div" style="width: 900px; height: 500px;"></div>
</body>
